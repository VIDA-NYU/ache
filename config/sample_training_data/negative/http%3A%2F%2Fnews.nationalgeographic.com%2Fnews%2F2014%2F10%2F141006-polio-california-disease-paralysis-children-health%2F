        <script type="text/javascript">
            isHTTPS = !!document.location.protocol.match('https:');

            // Set variable to the window as we need to reference it in a few other locations on this page
            // Need to see if we need to load assets for HTTPS sites
            window.globalnavURL = (isHTTPS ? "https://www-s.nationalgeographic.com/wpf/" : "//images.nationalgeographic.com/wpf/");

            if (!$("link[href='" + window.globalnavURL + "sites/themes/global/c/global_nav.css']").length) {
                if (document.createStyleSheet) {
                    // Covers all IE, but needed to load the CSS in IE8
                    document.createStyleSheet(window.globalnavURL + 'sites/themes/global/c/global_nav.css');
                } else {
                    console.log("Loading the global nav css");
                    $('<link href="' + window.globalnavURL + 'sites/themes/global/c/global_nav.css" media="screen" rel="stylesheet">').appendTo("head");
                };
            };
        </script>

    

    
