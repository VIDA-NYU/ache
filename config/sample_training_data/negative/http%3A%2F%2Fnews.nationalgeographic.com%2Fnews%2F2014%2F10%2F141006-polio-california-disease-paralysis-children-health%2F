                $(item).attr('href', $(item).attr('href') + '?source=newsbundlearticles') 
            });
        });
    </script>


    
        <script type="text/javascript" src="//images.nationalgeographic.com/wpf/CACHE/js/9a5b343d5797-cb1414755311.js" charset="utf-8"></script>
        <script type="text/javascript">
            //<![CDATA[
            $(document).ready(function(){
                if(getURLParam('from_bing') != 'null'){
                    $('#headerboard').prepend("<div style='width:997px;height:40px;margin-bottom:10px;background-color:#000;'><a href='http://www.bing.com' title='Bing'><img src='http://s.ngm.com/img/bing-promo.jpg' alt='Bing' /></a></div>");
                }

                function getURLParam(name) {
                    return unescape((RegExp(name + '=' + '(.+?)(&|$)').exec(location.search)||[,null])[1]);
                }
            });
            //]]>
        </script>
