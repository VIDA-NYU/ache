<script type="text/javascript">
  
$(document).ready(function() {var currNav = document.getElementById("guidenav_rdr");
var ctlinks = currNav.getElementsByTagName("a");
var currenturl = new RegExp(s_furl + "$", "gi");
for (var i=0; i<ctlinks.length; i++) {
var linkurl = ctlinks[i].getAttribute("href");
if (linkurl.search(currenturl) >=0) {
addClassName(ctlinks[i].parentNode, 'currentPage');
}
}
	});
  
  </script>
