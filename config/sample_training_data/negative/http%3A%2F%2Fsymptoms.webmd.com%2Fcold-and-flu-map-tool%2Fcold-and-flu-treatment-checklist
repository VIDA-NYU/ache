<script type="text/javascript">
$(function(){
	var $form = $('#mapPromoSearchForm'),
		$input = $("#geocodeSearch", $form);

	// geocode search
	$input.blur(function() {
		if ($input.val() === '') {
			$input.val($input.attr("title"));
		}
	}).focus(function() {
		if ($input.val() === $input.attr("title")) {
			$input.val('');
		}
	}).val($input.attr("title"));

	$form.on('submit', function(e) {
		wmdTrack('prm-prgmtoc');
		if ( $input.val() === $input.attr("title") ) {
			location = '/symptoms/cold-and-flu-map-tool/default.htm';
		} else {$input.val()
			location = '/symptoms/cold-and-flu-map-tool/default.htm?loc=' + $input.val();
		}
	})
});
</script></div>
