<script type="text/javascript">
if (typeof webmd.m.productSolutionsModule === 'object') {
	$('#sb-1181-ans-01').webmdPSM();
	webmd.debug('.webmdPSM extension initialized.');
} else {
	/* 	GK - Immediate function to handle PSM functionality - adding
		until external JS has fully propagated through the site */
	(function() {
		webmd.debug('ERROR: Failed to load .webmdPSM extension.');
		var productSolutionsModule = {
			init: function(psm){
				var that, meta, modID, tabIndex;
				that = this;
				that.psm = psm;
				/* look for keyword trigger to open accordion */
				meta = $('meta[name=keywords][content*=accordion_box_]');
				if (meta[0]) {
					tabIndex = meta[0].content.match(/accordion_box_(\w*)/)[1]-1;
					that.showTab(tabIndex, false);
				}
				/* add click event */
				$(".tab_header h3 a", psm).click(function() {
					var index = $(this).parents("li").index();
					return that.showTab(index, true);
				});
				/* Attach click event to disclosure */
				that.disclosure = $(".disclosure", psm);
				$("div.content div.header p a, .disclosure a.close, .disclosure", psm).click(function() {
					return that.toggleDisclosure();
				});
				return that;
			},
			showTab: function(index, bi_click) {
				var tab = $("ul.tabs > li:eq("+index+")", this.psm);
				$(".tab_content, .tab_header h3", this.psm).removeClass("show_tab");
				$(".tab_content, .tab_header h3", tab).addClass("show_tab");
				/* if true, trigger bi_click */
				if (bi_click) {
					wmdPageLink(tab.attr("id") + "-h");
				} 
				return false;
			},
			toggleDisclosure: function () {
				$(this.disclosure).toggleClass("visible");
				return false;
			}
		}		
		productSolutionsModule.init('#sb-1181-ans-01');
	})();
}			
			
			</script></div>
