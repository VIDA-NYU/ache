<script type="text/javascript">
(function() {
	var firstRun = true;
	$('img', '#attribution_rdr').add(window).load(function() {
		if(firstRun) {
			// calculate heights
			var $att = $('#attribution_rdr'),
				headerHeight = $('#header').outerHeight(),
				attHeight = $att.outerHeight(),
				heightDifference;

			if (!headerHeight) {
				return;
			} else {
				headerHeight -= 12;
			}
			
			if($('.ed_disclaimer')[0]) {
				headerHeight = headerHeight + Math.floor($('.ed_disclaimer').outerHeight());
			}

			// difference in height
			heightDifference = Math.abs(headerHeight - attHeight);

			// vertically align logo and header based on heightDifference
			if(attHeight > headerHeight) {
				$('#header').css('padding-top', Math.floor(heightDifference / 2) + 'px');
			} else {
				$att.css({
					'padding-top': Math.floor(heightDifference / 2) + 'px',
					'padding-bottom': Math.floor(heightDifference / 2) + 'px'
				});
			}

			firstRun = false;
		}
	});
})();
