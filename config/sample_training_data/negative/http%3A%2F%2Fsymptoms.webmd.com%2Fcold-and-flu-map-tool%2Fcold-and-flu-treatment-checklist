                  var pageviewId = window.s_pageview_id || '';

                  var iframeURLOutOfPB = '//as.webmd.com/html.ng/transactionID=1872869858&tile=347036404&tug=&pug=__&site=2&affiliate=20&hcent=&scent=&pos=113&xpg=4068&sec=&amp;au1=1&au2=14|17|16|18|15|13|8&uri=%2fsymptoms%2fcold-and-flu-map-tool%2fcold-and-flu-treatment-checklist&artid=091e9c5e80d595cd&inst=0&amp;leaf=13824&segm=0&cc=80&tmg=&bc=_cust4_j10_&mcent=&micro=&pvid=' + pageviewId;

