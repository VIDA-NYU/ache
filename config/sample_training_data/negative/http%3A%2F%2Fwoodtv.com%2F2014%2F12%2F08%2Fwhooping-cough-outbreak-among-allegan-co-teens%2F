			s.prop25='Steve Kelso and 24 Hour News 8 web staff';
			s.prop41='news:allegan-county:whooping-cough-outbreak-among-allegan-co-teens:detail';
			s.prop42='';
			s.prop55='http://woodtv.com/2014/12/08/whooping-cough-outbreak-among-allegan-co-prop23='';
			s.prop25='Steve Kelso and 24 Hour News 8 web staff';
			s.prop41='news:allegan-county:whooping-cough-outbreak-among-allegan-co-teteens/';
			s.prop56=s.linGetScreenSize();
			s.prop57=s.linGetRenderedExperience();
			s.prop58=s.linGetLayoutName();
			s.hier1='news,allegan-county';		}
		s.doPlugins=s_doPlugins;

		s.trackingServer="lintv.122.2o7.net";

		/************************** PLUGINS SECTION *************************/
		s = lin_aa_plugins(s);

		/****************************** MODULES *****************************/
		s = lin_aa_modules(s);
		s.loadModule("Media");
		s.Media.autoTrack=false;
		s.Media.trackWhilePlaying = true;
		s.trackMilestones = "25,50,75";
		s.Media.trackVars="prop29,prop30,prop31,prop32,eVar17,eVar18,eVar19";
		s.Media.trackEvents="None";
		s.loadModule("Integrate");

		var s_code=s.t();if(s_code)document.write(s_code);
		</script>
		

