                    <option value="/understanding/analytes/beta-microglob/" title="Beta-2 Microglobulin Tumor Marker">Beta-2 Microglobulin Tumor Marker</option>
                
                    <option value="/understanding/analytes/co2/" title="Bicarbonate">Bicarbonate</option>
                
                    <option value="/understanding/analytes/bilirubin/" title="Bilirubin">Bilirubin</option>
                
                    <option value="/understanding/analytes/blood-culture/" title="Blood Culture">Blood Culture</option>
                
                    <option value="/lab/bloodbank/" title="Blood Donation">Blood Donation</option>
                
                    <option value="/understanding/analytes/blood-gases/" title="Blood Gases">Blood Gases</option>
                
