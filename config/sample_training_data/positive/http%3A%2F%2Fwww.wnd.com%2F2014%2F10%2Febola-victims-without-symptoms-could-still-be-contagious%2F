    <script type="text/javascript">
        function getRandomInt(min, max) {
            return Math.floor(Math.random() * (max - min + 1)) + min;
        }
        var rotation = 30;
    
        // Display the signup form if they haven't signed up according to the ad rotation
        if ($.cookie('newsletter_signed_up') || rotation > getRandomInt(0,100)) { 
            $('body').prepend("<div id='div-gpt-ad-thinFullWidth' style='height: 27px; width:976px;'></div>");   
            googletag.cmd.push(function() { googletag.display('div-gpt-ad-thinFullWidth'); });         
        } else {
            $('body').prepend("<div id='newsletter-signup-form' class='desktop-only'><div class='loading'></div><div class='error' style='display: none;'><div class='error-text'>Could not complete your signup.  <span class='reason'></span></div></div><div class='success-new success-update' style='display: none;'><h1>Thank You!</h1><p>We've sent an email to <strong><span class='email'></span></strong>. Please click the link in this email to complete your signup.<br /><a id='not-seeing-email' href='javascript:;'>Not seeing the email?</a></p><p class='note'><strong>Note:</strong> WND does this to protect your privacy. Until you complete this process, WND.com will not send you any email alerts. </p></div><form method='post' data-form-target=''><div class='fields'><input type='hidden' name='emv_tag' value='9590D1440100003A' /><input type='hidden' name='emv_ref' value='EdX7CqkmjWb08SA9MOPQKK7eLUV6aKzH_jjce6k2WMOoK7o' /><input type='hidden' name='ALERTS_PERIODIC' value='X' /><input type='hidden' name='f-ALERTS_DAILY' value='X' /><input type='hidden' name='f-SOURCE' value='Page top signup' /><input type='hidden' name='f-DOUBLE_OPT' value='X' /><input type='hidden' name='f-OFFERS_STORE' value='X' /><input type='hidden' name='f-OFFERS_3P' value='X' /><div class='main-row'><label for='newsletter-email' class='form-title'><span>Subscribe to </span><span class='logo'>WND</span><span style='color: #fff;'>'s News Alerts</span></label><input type='email' name='f-EMAIL' id='newsletter-email' class='required' placeholder='Email' /><span class='required-icon'>*</span></div><div class='hide-first'><p class='newsletter-desc'>Subscribe to WND's Email Alerts and you'll never worry about missing the hottest stories on the Internet. Your e-mail address and postal code will not be sold or abused, but we'll keep you up-to-date on the latest news, special offers and inside scoops from your favorite news source.</p><div class='additional-fields'><input type='email' name='confirm-email' class='required' placeholder='Re-type Email' id='newsletter-re-type-email' /><span class='required-icon'>*</span><div class='name-row'><input type='text' name='f-FIRSTNAME' placeholder='First Name' class='first-name' /><input type='text' name='f-LASTNAME' placeholder='Last Name' class='last-name' /></div><input type='text' name='f-POSTAL_CODE' class='required' placeholder='Postal Code' /><span class='required-icon'>*</span></div></div><div class='submit-button'><button id='newsletter-submit'>Subscribe</button></div><a href='javascript:;' class='already-subscribed'>Already Subscribed?</a><div class='legend'><span class='icon'>*</span> Required field</div></div></form></div>");
            
            var notSeeingEmail = $('<div id="not-seeing-email-window" style="display: none;"><div class="close">&times;</div><h3>Troubleshooting E-mail Verification</h3>If you have trouble receiving your Inbox Verification E-mail, please try the following:<ul><li><strong>Wrong Address?</strong> Make sure that you have typed in the correct e-mail address (displayed above). You may correct any mistakes by returning to the Profile Center and editing your e-mail address.</li><li><strong>Junk Mail Filter?</strong> Make sure that the Verification E-mail did not get filtered into the junk mail folder of your e-mail application.</li><li><strong>Blocked by ISP?</strong> In some cases e-mails may be accidentally blocked by anti-spam technology used by your Internet Service Provider (ISP). Contact your ISP and ask them if they are blocking e-mail sent by "WND".</li></ul></div>');
            
            $('#not-seeing-email').after(notSeeingEmail);
            
            notSeeingEmail.find('.close').click(function() {
               $(this).parent().fadeOut(); 
            });
            
            $('#not-seeing-email').click(function() {
                $('#not-seeing-email-window').fadeIn();  
            });
            
            
            // For expanding
            $('#newsletter-signup-form .hide-first').css({position: 'absolute', visibility: 'hidden', display: 'block' });
            var hiddenHeight = $("#newsletter-signup-form .hide-first").height();
            $("#newsletter-signup-form .hide-first").css({ position: 'static', visibility: 'visible', height: 0});
            $('#newsletter-submit').attr('disabled','disabled');
            $('#newsletter-signup-form form').attr('disabled','disabled');
            
            // Delay is required because of the way IE works
            $("#newsletter-email").click(function() {
                $("#newsletter-signup-form .hide-first").animate({
                    height: hiddenHeight
                }, 300, function() {
                    $("#newsletter-signup-form .hide-first").css('overflow','visible');
                    $('#newsletter-submit').removeAttr('disabled');
                    $('#newsletter-signup-form form').removeAttr('disabled');
                                    
                    $("#newsletter-signup-form .required-icon").each(function(i, el) {
                        setTimeout(function() {
                            $(el).fadeIn(200);
                        }, (i * 300));
                    });
                
                    setTimeout(function() {
                        $('#newsletter-signup-form .legend').fadeIn(200);
                    }, ($("#newsletter-signup-form .required-icon").length * 300));
                });
            });
                      
            
            // Check for proper email formatting
            $('#newsletter-email').blur(function() {
                if (!$(this).val().match(/.+\@.+\..+/i) && $(this).val().length > 0) {
                    if (!$(this).hasClass('error')) {
                        $(this).addClass('error');     
                        var p = $(this).position();           
                        $(this).after('<div class="field-error" style="margin-left: ' + p.left + 'px"><div class="field-error-arrow"></div><div class="field-error-text">Not a valid email format. Did you forget something?</div></div>');
                    }
                    $('#newsletter-submit').attr('disabled','disabled');
                    $('#newsletter-signup-form form').attr('disabled','disabled');
                    
                } 
            });
            $('#newsletter-email').keyup(function(){
                if ($(this).val().match(/.+\@.+\..+/i)) {
                    if ($(this).hasClass('error')) {
                        $(this).removeClass('error');
                        if ($(this).next().hasClass('field-error'))
                            $(this).next().remove();
                    }
                    $('#newsletter-submit').removeAttr('disabled');
                    $('#newsletter-signup-form form').removeAttr('disabled');              
                }
            });
            
            // let a person know when the emails don't match
            $('#newsletter-re-type-email').keyup(function(){
                var pattern = new RegExp('^' + $(this).val(), 'i');
            
                if (!$('#newsletter-email').val().match(pattern)) {
                    if (!$(this).hasClass('error')) {
                        $(this).addClass('error');                
                        var p = $(this).position();           
                        $(this).after('<div class="field-error" style="margin-left: ' + p.left + 'px"><div class="field-error-arrow"></div><div class="field-error-text">Email doesn\'t match</div></div>');
                    }
                    $('#newsletter-submit').attr('disabled','disabled');
                    $('#newsletter-signup-form form').attr('disabled','disabled');
                } else {
                    if ($(this).hasClass('error')) {
                        $(this).removeClass('error');
                        if ($(this).next().hasClass('field-error'))
                            $(this).next().remove();
                    }
                    $('#newsletter-submit').removeAttr('disabled');
                    $('#newsletter-signup-form form').removeAttr('disabled');
                }
            });
            
            $('#newsletter-signup-form input').keyup(function(){
                if ($(this).hasClass('clear-error-on-keyup')) {
                    $(this).removeClass('clear-error-on-keyup').removeClass('error');   
                 
                    if ($(this).next().hasClass('field-error'))
                        $(this).next().remove();
                }
            });
            
            $('#newsletter-signup-form').submit(function(e) {
    
                
            });
            
        } 
    </script>
