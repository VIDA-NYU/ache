	<script src="http://s.telegraph.co.uk/toolbar/js/pinConfigV4.js" type="text/javascript" ></script>
	<script type="text/javascript">

	var subscribtionPinWiget = (function(){

		var settings = {
			headline: "",
			imageUrl: "",
			bulletPoints: [null,null,null],
			buttonText: "",
			containerBgColor: "",
			buttonBgColor: ""
		};

		var trackingId = "";
		var destinationUrl = "";

		var init = function(widgetSettings,widgetTrackingId,widgetDestinationUrl){
			settings = widgetSettings;
			trackingId = widgetTrackingId;
			destinationUrl = widgetDestinationUrl;

			if(showSubscriptionPin() === true) {
				$('#tmglSite').append(buildSubscriptionPin());

				$('#subscriptionPinCloseButton').click(function() {
					$('#subscriptionPin').fadeOut(1000);
					//do not show for 4 days
					$.cookie('tmgSubscriptionPinClosed', 'true', { expires: 4, path: '/', domain: '.telegraph.co.uk'});
					//tracking code for GA & webtrends
					 _gaq.push(['_trackEvent', 'subscription_pin', 'click_close', 'version_1']);
					dcsMultiTrack('WT.dl','53','DCSext.navigation','PIN'+trackingId+'_Close');
				});

				var displayed = false;
				$(window).scroll(function() {
					if ((displayed === false) && isOutOfView(document.getElementById("tmglMenu"))) {
						$('#subscriptionPin').fadeIn(1000);
						displayed = true;
						//do not show more than once a day
						$.cookie('tmgSubscriptionPinDisplayed', 'true', { expires: 1, path: '/', domain: '.telegraph.co.uk'});
						//tracking code for GA & webtrends
						_gaq.push(['_trackEvent', 'subscription_pin', 'display_open', 'version_1']);
						dcsMultiTrack('WT.dl','53','DCSext.navigation','PIN'+trackingId+'_Display');
					}
				})
			}
		};

		var isOutOfView = function(el){
			var isOutOfView = el.getBoundingClientRect().bottom <= 0;
			return isOutOfView;
		};

		// get the various data (segments and userid) required and store in the tmp object, we're after the Krux user and also the segment ids.
		// NB: Keux segments are stored under the kxsegs cookie or localStorage name and are stored as a comma delimited string
		// Once we have a list of segments we need to loop through them to see if the target segment 'oojk8yna1' is present
		var isKruxUser = function(){
			var segments;
			var result = false;

			if(window.localStorage != undefined){
				// when localstorage is available do this.
				segments = window.localStorage.kxprvx_segs;
			} else {
				// when localstorage not supported use cookies
				segments = $.cookie('kxprvx_segs');
			}
			// now lets get each segment and create the final string top use in the tag.
			if(segments !== null && segments !== undefined){
				var tmpSegs = segments.split(",");
				for(i in tmpSegs){
					if(tmpSegs.hasOwnProperty(i) && tmpSegs[i]!=""){
						//  check to see if this seg = regsitered user
						if(tmpSegs[i]=="oojk8yna1"){
							result = true;
						}
					}
				}
			}
			return result;
		};


		var showSubscriptionPin = function(){

			if($.cookie('tmgSubscriptionPinClosed') === 'true') {
				return false;
			}

			if($.cookie('tmgSubscriptionPinDisplayed') === 'true') {
				return false;
			}

			if(isKruxUser()) {
				return false;
			}

			if(samPageUser.loggedIn === false) {
				return true;
			}

			if(samPageUser.loggedIn === true && getCookieValue("tmg_p13n","subscriber") === 'false') {
				return true;
			}

			return false;
		};

		var buildSubscriptionPin = function(){

			var subLink = "";

			if((typeof destinationUrl !== 'undefined') && (destinationUrl !== null) && (destinationUrl.length > 0 )){

				subLink = destinationUrl;

			} else {

				if(typeof  samSubscribeLink != 'undefined') {
					subLink = samSubscribeLink;
				}

				if(typeof  subscriptionsLink != 'undefined') {
					subLink = subscriptionsLink;
				}

				subLink = subLink + '?WT.mc_id='+trackingId;
			}

			bulletPointsHtml = $('<ul></ul>');

			$.each(settings.bulletPoints,function(key,value){
				if(key > 2 ){return false;}
				if(value!==null){
					bulletPointsHtml.append('<li><img src="/template/ver1-0/i/subscriber/Bullet_icon_pin.png"/>'+value+'</li>');
				}
			});

			var html='<div id="subscriptionPin" style="background-color:'+settings.containerBgColor+'">'+
						'<div id="subscriptionPinInnerDiv">'+
							'<img id="subscriptionPinCloseButton" src="/template/ver1-0/i/subscriber/Close_12x12.png"/>'+
								'<a id="subscriptionPinLink" href="' + subLink + '">'+
									'<div>'+
										'<img src="/template/ver1-0/i/subscriber/Telegraph_logo_151x24.png"/>'+
										'<p>'+settings.headline+'</p>'+
										'<hr>'+
										'<div id="subscriptionPinDesc">'+
										'<div id="subscriptionPinImg"><img src="'+settings.imageUrl+'"/></div>'+
					                    '<div id="subscriptionPinBulletPoints">'+bulletPointsHtml.wrap("<div></div>").parent().html()+'</div>'+
										'</div>'+
										'<div id="subPinBtn" style="background-color:'+settings.buttonBgColor+'">'+
											'<p>'+settings.buttonText+'</p>'+
										'</div>'+
									'</div>'+
								'</a>'+
							'</div>'+
						'</a>'+
					'</div>'+
				'</div>';

		return html;
		};

		//public API
		return {
			settings:settings,
			init: init
		}
	})();

	$(document).ready(function() {
		
			var pinTrackingId = "602860";
			
			var pinTrackingIdNonUK = "602858";
			


		if(typeof subscriptionPinSettings !== 'undefined'){
			destinationUrl = null;
			destinationUrlNonUk = null;
			
				if($("meta[name=DCSext.akamigeo]").attr('content') === 'GB') {
					
					destinationUrl = "http://www.telegraph.co.uk/subscriptions/2/?WT.mc_id=602860";
					
					subscribtionPinWiget.init(subscriptionPinSettings.UK,pinTrackingId,destinationUrl);
				}
			
				if($("meta[name=DCSext.akamigeo]").attr('content') !== 'GB') {
					
					destinationUrlNonUk = "http://www.telegraph.co.uk/subscriptions/international/international-2/?WT.mc_id=602858";
					
					subscribtionPinWiget.init(subscriptionPinSettings.nonUK,pinTrackingIdNonUK,destinationUrlNonUk);
				}
			
		}

	});

	</script>
