<!-- fi:/csp/mediapool/sites/shared/assets/csp/includes/mobileView.csp -->


<script type="text/javascript">
	var mobileViewOn = 0; //flag to know if we need to change views or if already there
	
	$(document).ready(function() {
	
		// ***********************************************************************************************
		//  all changes that need to be made when the window becomes mobile view 767 px or less
		// ***********************************************************************************************
		
		function showMobileView() {
			$(".signIn").hide();
			$(".rtRail").hide();
			$(".logo").hide();
			$(".subscriptions").hide();
			$(".theBulletin").hide();
			$(".footer-subscribe").hide();
			$(".breadcrumb").hide();
			$("#homeCarousel").hide();
			
			$('.navbar-brand').show(); 
			$('.moreContent').show();
			
			
			
			$("<div class='row areaTitle mobileLogin'><a href='https://www.syncaccess.net/wc/bb/account/logon?returnUrl=http%3A%2F%2Fwww.bendbulletin.com%2F'>Subscriber Sign In &raquo</a></div>").insertBefore(".subscribeBlob");
			
			$("<div class='row areaTitle mobileClass'><a href='/classifieds/'>Classifieds &raquo</a></div>").insertAfter(".mobileNavBottom");
			$("<div class='row areaTitle mobileSS'><a href='/slideshows/'>Slide Shows &raquo</a></div><br />").insertAfter(".areaTitle:last");
			$("<div class='row areaTitle mobileObits'><a href='/obituaries/'>Obituaries &raquo</a></div><br />").insertAfter(".areaTitle:last");
			$("<div class='row areaTitle mobileEvents'><a href='/events/'>Events &raquo</a></div>").insertAfter(".areaTitle:last");
			$("<div class='row areaTitle mobileReplica'><a href='/replicaPage/'>Replica Edition &raquo</a></div>").insertAfter(".areaTitle:last");
			$("<div class='row areaTitle mobileWeather'><a href='/weather/'>Weather &raquo</a></div><br />").insertAfter(".areaTitle:last");
			$("<div class='row areaTitle mobileSubscribe'><a href='/Subscribe/'>Subscribe / Manage Account &raquo</a></div>").insertAfter(".areaTitle:last");
			//$("<div class='row areaTitle mobileSubscribe'><a href='/Subscribe/'>Subscribe / Manage Account &raquo</a></div>").insertAfter(".areaTitle:last");
			
			// subscribe page - add subscribe button and price table to top of page
			$("<h5 class='mobileSubscribeText'>Get the best in Central Oregon News, Sports, and Community Life</h5><form id='fmResultsSearch' class='form-search mobileSubscribeButton' action='https://tbbiservices.dticloud.com/cgi-bin/cmo_cmo.sh/custservice/web/zipcampaign?siteid=bull' method='get' name='fmResultsSearch' target='_top'><button class='btn btn-primary btn-sm' type='submit'>Subscribe</button></form>").insertAfter(".subSignIn");
			$("<div class='subRateChart'><h3>Subscription Options:</h3><table><tbody><tr><th colspan='2'>E-Edition:</th></tr><tr><td style='padding-right: 20px;'>1 month</td><td>$13.00</td></tr><tr><td style='padding-right: 20px;'>3 months</td><td>$39.00</td></tr><tr><td style='padding-right: 20px;'>6 months</td><td>$78.00</td></tr></tbody></table><table><tbody><tr><th colspan='2'>Bulletin home delivery + E-Edition:</th></tr><tr><td>1 month</td><td>$17.00</td></tr><tr><td>3 months</td><td>$51.00</td></tr><tr><td>6 months</td><td>$102.00</td></tr></tbody></table></div>").insertAfter(".mobileSubscribeButton");
			
			$('.mobileFooterQuickLinks').css({"float":"left", "line-height":"1.7em"});
			$('.mobileFooterPhotos').css({"padding-left":"45%", "line-height":"1.7em"});
			$('.mobileFooterAddthis').show();
			
			$('li:contains("Submit an Event")').hide();
			$('li:contains("Parade Magazine")').hide();
			$('li:contains("ODOT Web Cams")').hide();
			//$('li:contains("Obituaries")').hide();
			$('li:contains("Oregon Lottery")').hide();
			$('li:contains("Restaurant Guide")').hide();
			
			// shrink button text for mobile section view
			$('.startSectionSearch').html('More articles in \'Home\'');
			
			// if on weather section, show the ski cams
			$('.rtRailSkiConditions').insertAfter(".webCams:last");
			
			// hides for story view
			$('.print').hide();
			$('.addthis_button_compact').hide();
			$('.addthis_button_stumbleupon').hide();
			$('.addthis_button_google').hide();
			$('.addthis_separator').hide();
			$('.addthis_trendingcontent').hide();
			$('.mobileTrendingContent').hide();
			$('#toolbox').insertAfter('.article-content');
			$('#toolbox').show();
			$('#toolbox').css({"width":"95%", "float":"none"});
			
			mobileViewOn = 1;
		}
		
		// ***********************************************************************************************
		//  all changes that need to be made when the window becomes normal view 768 px or more
		// ***********************************************************************************************
		function showNormalView() {
			$(".signIn").show();
			$(".rtRail").show();
			$(".logo").show();
			$("#homeCarousel").show();
			$(".subscriptions").show();
			$(".theBulletin").show();
			$(".footer-subscribe").show();
			$(".moreContentBlock").show();
			$(".breadcrumb").show();
			$('.moreContent').hide();
			$('.navbar-brand').hide();
			$('#mobileNav').hide();
			//$('.subNavBar').show();
			
			$('.mobileClass').remove();
			$('.mobileSS').remove();
			$('.mobileEvents').remove();
			$('.mobileReplica').remove();
			$('.mobileWeather').remove();
			$('.mobileSubscribe').remove();
			$('.mobileObits').remove();
			
			// subscribe page
			$('.mobileSubscribeButton').remove();
			$('.mobileSubscribeText').remove();
			$('.subRateChart').remove();
			
			//change button back to long text for section view
			$('.startSectionSearch').html('Click here to find more stories in \'Home\'');
			
			// if on weather section, remove the ski cams from bottom
			$('.rtRailSkiConditions').insertAfter(".classFeat");
			
			// shows for story view
			$('.print').show();
			$('.addthis_button_compact').show();
			$('.addthis_button_stumbleupon').show();
			$('.addthis_button_google').show();
			$('.addthis_separator').show();
			$('.addthis_trendingcontent').show();
			$('.mobileTrendingContent').show();
			$('#toolbox').insertBefore('.article-content');
			$('#toolbox').css({"width":"35%", "float":"right"});
			
			mobileViewOn = 0;
		}
		
		// ***********************************************************************************************
		//  gets called on window load to see if we need to be in mobile view if so, make changes
		// ***********************************************************************************************
		var boxWidth = $(window).width();

		if($(window).width() <= 767) {
			showMobileView();		
		}
		
		// ***********************************************************************************************
		// 	really code for desktop if they resize below mobile view size or go back to normal size
		// ***********************************************************************************************
		$(window).resize(function (event) {
			if($(window).width() <= 767){
				if(mobileViewOn == 0) {
					showMobileView();
				}
			} else {
				if(mobileViewOn == 1) {
					showNormalView();
				}
			}
			
		});
		
		// ***********************************************************************************************
		//  code to create mobile accordion drop down
		// ***********************************************************************************************
		$(".moreContent").click(function () {
			
			$moreContent = $(this).parent().parent().parent();
			$moreContentBlock = $moreContent.next().next().next().next();
			$theImage = $(this).children('img');
			
			$moreContentBlock.slideToggle(500, function () {
				
				if ($moreContentBlock.is(":visible")) 
				{
					$theImage.attr('src', 'http://statics.bendbulletin.com/images/redMinus.png');
				}
				else
				{
					$theImage.attr('src', 'http://statics.bendbulletin.com/images/greenPlus.png');
				}
				
			});
		});		 
	});

</script>
