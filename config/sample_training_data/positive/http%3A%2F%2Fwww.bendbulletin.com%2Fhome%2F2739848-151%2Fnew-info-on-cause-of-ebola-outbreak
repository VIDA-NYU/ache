		function purchasePhoto(val, ours) {
			
			if (ours == "true")
			{
				val = val.replace(/ /g, "+");
				val = val+".jpg";
				//alert("Image: "+val+" is copyrighted");	
				var url = 'http://bendbulletin.mycapture.com/mycapture/lookup.asp?originalname='+val+'&backurl=http://www.bendbulletin.com';
				$('#popupcontent > .purchaseLink > a').attr("href", url);
					
				//var popUp = document.getElementById("popupcontentStory");
				var winWid = $('.fullStoryLayout').width();
				console.log("winwid2 = "+winWid);
				var myx = (winWid / 2) - 200;
				var p = $('.fullStoryLayout');
				var position = p.position();
				var myy = position.top + 50;
				console.log("myy = "+myy);
				
				$('#popupcontent').css("top", myy+"px");	
				$('#popupcontent').css("left", myx+"px");
				$('#popupcontent').css("width", "400px");
				$('#popupcontent').css("height", "200px");
				
				$('#popupcontent').css("visibility", "visible");
			}
			else
			{
				alert("This photo is copyrighted by someone other than The Bulletin and therefore may not be downloaded or purchased.");
			}
		}
			
		function hidePopup() {
			$('#popupcontent').css("visibility", "hidden");
		}
		
	</script>
	


