	function dggstc_checkShareCount() {
		var cnt = jQuery('#dggsharethiscustom_sharecnt').html();
		if(cnt=='-'||cnt=='') return;
		window.clearTimeout(window.timeoutID);
		if((cnt!='new' && cnt>100)|| cnt.slice(-1)=='K') {
			jQuery('.dggsharethiscustom .counter .value').html(cnt);
			jQuery('.dggsharethiscustom .counter').fadeIn();
		}
	}
	
