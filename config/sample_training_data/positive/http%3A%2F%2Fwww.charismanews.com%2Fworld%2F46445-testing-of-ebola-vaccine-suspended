				function ckAdBl(){if(adBladeUnits.length==0)return;var e=jQuery("body").width();if(desktopSeen&&e>=980)return;if(e<980){jQuery("#adblade").show();jQuery("#adBladeDeskArt").hide()}else{desktopSeen=true;adBladeUnits.forEach(function(e,t,n){if(e.layout!="desktop")return;window.adblade_cid=e.cid;window.adblade_ad_width=e.ad_width;window.adblade_ad_height=e.ad_height;window.adblade_ad_host=e.ad_host;window.adblade_tag_type=e.tag_type;(function(){var t=window,n=("https:"==document.location.protocol?"https":"http")+"://"+(t.adblade_ad_host?t.adblade_ad_host:"web.adblade.com")+"/",n=n+"impsc.php?",r=t.adblade_ad_width,i=t.adblade_ad_height,s=escape(t.top.location==t.location?document.location:document.referrer),n=n+("cid="+t.adblade_cid),n=n+("&url="+s.substring(0,512)),n=n+(t.adblade_rtbid?"&rtbid="+t.adblade_rtbid:""),n=n+(t.adblade_subid?"&subid="+t.adblade_subid:""),n=n+("&rnd="+(new Date).getTime()),n=n+("&output="+(t.adblade_output?t.adblade_output:"html")),n=n+(t.adblade_tp_url?"&tpUrl="+t.adblade_tp_url:"");"adblade_tag_type"in t||(t.adblade_tag_type=1);if(1==t.adblade_tag_type){jQuery(e.placeholder).append('<iframe name="adblade_ad_iframe" width="'+r+'" height="'+i+'" frameborder="0" src="'+n+'" marginwidth="0" marginheight="0" vspace="0" hspace="0" allowtransparency="true" scrolling="no"></iframe>')}})()})}}var desktopSeen=false;window.addEvent("domready",function(){ckAdBl();jQuery(window).resize(function(){ckAdBl()})})
				
	
