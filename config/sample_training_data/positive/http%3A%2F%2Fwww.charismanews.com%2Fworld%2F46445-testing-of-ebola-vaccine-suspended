<script src="http://www.charismaadvertising.com/js/detect-zoom.min.js"></script>
<script>
var lastZoom = detectZoom.zoom();
function checkZoomChange() {
	var curZoom = detectZoom.zoom();
	if(lastZoom < curZoom) {
		// they zoomed in
		jQuery("#toBtnLeft").hide();
		jQuery("#toBtnRight").hide();
	} else if(lastZoom > curZoom) {
		// they zoomed out
		jQuery("#toBtnLeft").show();
		jQuery("#toBtnRight").show();
	} else {
		return;
	}
	lastZoom = curZoom;
}
window.setInterval(checkZoomChange, 1000);

// for fixed backgrounds with floating invisible buttons
jQuery(window).scroll(function() {
	if(jQuery('body').css('background-attachment')=='fixed') {
		var d = jQuery(window).scrollTop();
		if(jQuery("#toImageMapLeft").is(':visible')) {
			// 29 = original "top" offset (px)
			jQuery("#toImageMapLeft").css('top',29+d);
		}
		if(jQuery("#toImageMapRight").is(':visible')) {
			jQuery("#toImageMapRight").css('top',210+d);
		}
	}
});


</script>

