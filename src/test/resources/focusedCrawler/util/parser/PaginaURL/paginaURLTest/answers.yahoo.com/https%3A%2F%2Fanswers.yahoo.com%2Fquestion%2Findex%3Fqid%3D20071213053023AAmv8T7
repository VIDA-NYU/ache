                        LREC: {wcpx: 1, hcpx: 1, dest: 'tgtLREC', clean: 'boxLREC' ,fdb: { "on": 1 }   ,metaSize: true},
LDRB: {wcpx: 1, hcpx: 1, dest: 'tgtLDRB', clean: 'boxLDRB'  ,supports: {"exp-push": 1}  ,metaSize: true}
                    };
                    DARLA.config(DARLA_CONFIG);
                }
                if (window.DARLA && window.DARLA_CONFIG) {
                    DARLA.config(DARLA_CONFIG);
                    DARLA.render();
                    if(monAdEnabled && !monAd && !mastAd){
                        DARLA.event('renderLREC');
                    }
                }
                if(mastAd){
                    var ldrb = Y.one('#ya-darla-LDRB');
                    var lrec = Y.one('#ya-darla-LREC');
                    if(ldrb) {
                        ldrb.addClass('D-n');
                    }
                    if(lrec){
                        lrec.addClass('D-n');
                    }
                }
                else {
                    Y.log("No MAST ad");
                }
           });
      var profileCardNode = Y.one('#ya-profile-card-wrapper');
      new Y.YA.Q.ProfileCard({
       'node' : profileCardNode,
       'userGuid' : ''
      });
    var qnSort = new Y.YA.Q.Sort({'node': '#ya-qn-sort'});
    var ansActions = new Y.YA.Q.AnsActions({
      'node': '#ya-best-answer',
