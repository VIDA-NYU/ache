    });
      var askWidgetContainer = Y.one('#ya-askbar-holder .ask-widget');
      if(askWidgetContainer) {
        new Y.YA.AskWidgetController.Controller({
          'widget' : new Y.YA.AskWidget.Widget({
             'node': askWidgetContainer,
             'safeWordLengthToSearch' : 4
          }),
