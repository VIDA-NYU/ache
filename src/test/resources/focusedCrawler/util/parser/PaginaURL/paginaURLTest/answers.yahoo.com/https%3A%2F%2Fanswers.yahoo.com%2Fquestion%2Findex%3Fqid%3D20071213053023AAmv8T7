      'pending': [],
      'nickname': '',
      'profileId': ''
    });
    var centerTwoColDiv = Y.one("#ya-center-two-col"),
        leftRailDiv     = Y.one("#ya-left-rail"),
        centerRailDiv     = Y.one("#ya-center-rail"),
        rightRailDiv     = Y.one("#ya-right-rail"),
        middleRailHeight  = 0,
        leftRailHeight    = 0,
        rightRailHeight  = 0;

    middleRailHeight = parseInt(centerRailDiv.getComputedStyle('height')) + (centerTwoColDiv ? parseInt(centerTwoColDiv.getComputedStyle('height')):0);
    leftRailHeight   = parseInt(leftRailDiv.getComputedStyle('height'));
    rightRailHeight   = parseInt(rightRailDiv.getComputedStyle('height'));

    new Y.YA.Module.AnswerBox({
      module: 'YAAnswerBoxModule',
      cookieName: 'QPageAddAnswer',
      postSubmit: false,
      node: Y.one('#ya-answer-box'),
